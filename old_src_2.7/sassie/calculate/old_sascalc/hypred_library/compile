#!/bin/bash

g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ./construct_prdf.cc -o build/temp.macosx-10.5-i386-2.7/construct_prdf.o
g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ../src/box.cc -o build/temp.macosx-10.5-i386-2.7/box.o
g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ../src/util.cc -o build/temp.macosx-10.5-i386-2.7/util.o
g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ../src/pRDF.cc -o build/temp.macosx-10.5-i386-2.7/pRDF.o
g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ../src/Atom.cc -o build/temp.macosx-10.5-i386-2.7/Atom.o
g++ -fno-strict-aliasing -fno-common -dynamic -arch i386 -DNDEBUG -g -O3 -arch i386 -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I../include/ -I/Library/Frameworks/Python.framework/Versions/7.3/lib/python2.7/site-packages/numpy/core/include -I/Library/Frameworks/Python.framework/Versions/7.3/include/python2.7 -c ../src/coor.cc -o build/temp.macosx-10.5-i386-2.7/coor.o



g++ -bundle -undefined dynamic_lookup -g -arch i386  build/temp.macosx-10.5-i386-2.7/construct_prdf.o build/temp.macosx-10.5-i386-2.7/util.o build/temp.macosx-10.5-i386-2.7/coor.o build/temp.macosx-10.5-i386-2.7/Atom.o build/temp.macosx-10.5-i386-2.7/box.o build/temp.macosx-10.5-i386-2.7/pRDF.o -o build/lib.macosx-10.5-i386-2.7/prdf.so -framework Python -g -O0
